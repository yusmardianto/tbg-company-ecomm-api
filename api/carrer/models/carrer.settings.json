{
  "kind": "collectionType",
  "collectionName": "carrers",
  "info": {
    "name": "Carrer"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "img": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    },
    "category": {
      "type": "string"
    }
  }
}
